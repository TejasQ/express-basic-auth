{
  "scripts": {
    "test": "nyc --reporter=html --reporter=text mocha --inspect --require babel-register --require source-map-support --recursive",
    "build": "babel src --out-dir js",
    "prepare": "npm run build",
    "watch": "babel --watch src --out-dir js",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "main": "js/index.js",
  "license": "MIT",
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "name": "@pubcore/authentication-rest",
  "version": "1.0.0",
  "description": "express middleware to for http basic authentication workflow",
  "directories": {
    "test": "test"
  },
  "keywords": [
    "basic",
    "auth",
    "authentication",
    "login",
    "workflow"
  ],
  "author": "pubcore",
  "dependencies": {
    "@pubcore/auth-user": "^1.0.0",
    "@pubcore/authentication": "^1.0.0",
    "basic-auth": "^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "coveralls": "^3.0.1",
    "eslint": "^4.19.1",
    "eslint-plugin-mocha": "^5.0.0",
    "express": "^4.16.3",
    "mocha": "^5.2.0",
    "moment": "^2.22.2",
    "nyc": "^12.0.2"
  }
